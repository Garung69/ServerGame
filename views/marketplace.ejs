<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">   
    <link href='https://unpkg.com/boxicons@2.1.2/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="css/mk.css">
    <title>Marketplace</title>
</head>

<body>
    <%- include('nav') %>
    <div class="containers">
        <div class="suvnav">
            <div class="navoptions">
                <button class="btn"><img src="data:image/png;base64,"/> Items</button>
                <button class="btn"><img src="data:image/png;base64,"/> Activity</button>
            </div>
        </div>
        <div class="filter">
            Filter
            <div class="category">
                <ul >
                    <li><input class="form-check-input" type="checkbox" name="" id=""> All</li>
                    <li><input class="form-check-input" type="checkbox" name="" id=""> Minted</li>
                    <li><input class="form-check-input" type="checkbox" name="" id=""> Un-Minted</li>
                </ul>
                
            </div>
            <button class="btn btn-outline-info my-2 my-sm-0" type="submit"><i
                class='bx bx-search bx-tada-hover'></i> Find</button>
            <a class="btn shownav btn-outline-success my-2 my-sm-0" href="#nav3"><i class='bx bx-chevron-up-circle' style='color:#f8f8f8'  ></i></a>
        </div>
        <div class="groupitems">
            <% for(var i = 1; i< 20; i++) {
                %>
                <a href="/nft">
                    <div id="items-<%= i%>" class="items">
                        <div class="productimg"><img src="/nftimage/2.png" width="273px" height="273px" alt=""></div>
                        <div class="productdes">ABCD</div>
                        <div class="productaction">NOT MINTED</div>
                    </div>
                </a>
            <% }%>
        </div>
    </div>
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://unpkg.com/boxicons@2.1.2/dist/boxicons.js"></script>
</body>

</html>
<script>
    window.onload = (translate) => {
        const items = 20;
        for(var i = 1; i<items; i++){
            let X = 0;
            let calX = i%4; 
            if(calX == 1){
                X = 0;
            }
            if(calX == 2){
                X = 286;
            }
            if(calX == 3){
                X = 572;
            }
            if(calX == 0){
                X = 858;
            }          
            let calY = (((i-1) - ((i-1) % 4)) / 4);
            let Y= calY * 449;  
            document.getElementById("items-"+i).style.transform = "translate("+X+"px, "+Y+"px)";
        }
    };

</script>